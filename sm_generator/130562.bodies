class UmlFinalState
!!!148354.cpp!!!	generate(inout machine : UmlClass, inout anystate : UmlClass, inout state : UmlState, inout body : string, in indent : string) : void
if (parent()->kind() == aState) {
  if (parent()->stereotype() == "machine")
    body += indent + "  stm._final();\n";
  else
    body += indent + "  stm" + static_cast<UmlState *>(parent())->path() +
            "._completion(stm);\n";
} else {
  UmlCom::trace("Error : a parent of FinalState is not a state");
  throw 0;
}
